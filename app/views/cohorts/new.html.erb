<h1>Create a new Cohort</h1>
<p>Find me in app/views/cohorts/new.html.erb</p>

<%= form_for(@cohort) do |f| %>
  <label>Cohort Name:</label>
  <%= f.text_field :name, placeholder: 'Name' %><br><br>
  <label>Start Date:</label>
  <%= f.date_field :start_date %><br><br>
  <label>End Date:</label>
  <%= f.date_field :end_date %><br><br>
  <%= f.submit 'Create Cohort' %>
<% end %>
